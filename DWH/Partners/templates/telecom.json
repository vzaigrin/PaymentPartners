{
  "fields": [
    {
      "name": "operation_ts",
      "type": "TIMESTAMP"
    },
    {
      "name": "operation_country",
      "type": "STRING",
      "values": {
        "list": ["Австрия", "Бельгия", "Великобритания", "Германия", "Греция", "Испания", "Италия", "Нидерланды"]
      }
    },
    {
      "name": "operation_city",
      "type": "STRING",
      "nullable": true
    },
    {
      "name": "card_bin",
      "type": "STRING",
      "values": {
        "list-file": "bins.csv"
      }
    },
    {
      "name": "card_number",
      "type": "STRING",
      "length": 4,
      "values": {
        "range": [0, 9999]
      }
    },
    {
      "name": "service",	
      "type": "STRING",
      "values": {
        "list": ["Услуга 1", "Услуга 2", "Услуга 3", "Услуга 4"]
      }
    },
    {
      "name": "payment_tariff",
      "type": "FLOAT64",
      "values": {
        "range": [100.0, 500.0]
      }
    },
    {
      "name": "payment_ps",
      "type": "FLOAT64",
      "values": {
        "range": [10.0, 20.0]
      }
    }
  ],
  "dds": [
    {
      "target": "bin",
      "source": "card_bin"
    },
    {
      "target": "card_number",
      "source": "card_number"
    },
    {
      "target": "operation_ts",
      "source": "operation_ts"
    },
    {
      "target": "operation_country",
      "source": "operation_country"
    },
    {
      "target": "operation_city",
      "source": "operation_city"
    },
    {
      "target": "payment_total",
      "source": "payment_tariff"
    },
    {
      "target": "payment_tariff",
      "source": "payment_tariff"
    },
    {
      "target": "payment_main_client",
      "source": "0.0"
    },
    {
      "target": "payment_ps",
      "source": "payment_tariff * (payment_ps / 100.0)"
    },
    {
      "target": "payment_partner",
      "source": "payment_tariff * (1 - (payment_ps / 100.0))"
    },
    {
      "target": "payment_other_client",
      "source": "0.0"
    }
  ]
}
